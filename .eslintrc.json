{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "no-console": "off",
        "linebreak-style" : "off",
        "quotes" : "off",
        "eol-last" : "off",
        "object-curly-newline": "off",
        "object-shorthand": "off"
    }
}
